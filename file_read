# This playbook uses the win_get_url module to download a simple HTML file for IIS

# List all the files in a forlder
template: src="{{ item }}" dest="generated/{{ inventory_hostname }}/{{ item | basename | replace('.j2', '')}}"
  delegate_to: 127.0.0.1
  with_fileglob: templates/*.j2

# Lee el contenido de un fichero
- hosts: all
  vars:
     contents: "{{ lookup('file', '/tmp/dtpks.tmp') }}"
  tasks:
     - debug: msg="El contenido de foo.txt is {{ contents }}"

 name: Include vars of stuff.yaml into the 'stuff' variable (2.2).
  include_vars:
    file: stuff.yaml
    name: stuff
